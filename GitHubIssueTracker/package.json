{
  "name": "githubissuetracker",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "test:watch": "yarn test -- --watch",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "clean": "rm -rf node_modules && yarn install && cd ios && rm -rf Pods && pod install && cd ../android && ./gradlew clean",
    "clean-android": "cd android && ./gradlew clean cleanBuildCache",
    "clean-ios": "cd ios && pod deintegrate && pod install"
  },
  "dependencies": {
    "@react-native-masked-view/masked-view": "^0.2.6",
    "@react-navigation/native": "^6.0.6",
    "@react-navigation/native-stack": "^6.2.5",
    "@react-navigation/stack": "^6.0.11",
    "@reduxjs/toolkit": "^1.7.0",
    "@types/date-fns": "^2.6.0",
    "@types/react-native-vector-icons": "^6.4.10",
    "@types/react-redux": "^7.1.20",
    "@types/redux-mock-store": "^1.0.3",
    "@types/styled-components": "^5.1.17",
    "@types/styled-components-react-native": "^5.1.3",
    "axios": "^0.24.0",
    "babel-plugin-styled-components": "^2.0.2",
    "date-fns": "^2.27.0",
    "formik": "^2.2.9",
    "react": "17.0.2",
    "react-native": "0.66.3",
    "react-native-gesture-handler": "^2.1.0",
    "react-native-reanimated": "^2.3.1",
    "react-native-safe-area-context": "^3.3.2",
    "react-native-screens": "^3.10.1",
    "react-native-vector-icons": "^9.0.0",
    "react-redux": "^7.2.6",
    "redux-saga": "^1.1.3",
    "styled-components": "^5.3.3",
    "typed-redux-saga": "^1.3.1"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.12.5",
    "@react-native-community/eslint-config": "^2.0.0",
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/jest-native": "^4.0.4",
    "@testing-library/react": "^12.1.2",
    "@testing-library/react-native": "^9.0.0",
    "@types/jest": "^26.0.23",
    "@types/react-native": "^0.66.4",
    "@types/react-test-renderer": "^17.0.1",
    "babel-jest": "^26.6.3",
    "eslint": "^7.14.0",
    "jest": "^26.6.3",
    "jest-styled-components": "^7.0.8",
    "metro-react-native-babel-preset": "^0.66.2",
    "react-dom": "^17.0.2",
    "react-test-renderer": "17.0.2",
    "redux-mock-store": "^1.5.4",
    "typescript": "^4.4.4"
  },
  "resolutions": {
    "@types/react": "^17",
    "ansi-regex": "^5.0.1",
    "styled-components": "^5"
  },
  "jest": {
    "preset": "react-native",
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/__mocks__/styleMock.js"
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "setupFiles": [
      "<rootDir>/jest/setup.ts"
    ],
    "setupFilesAfterEnv": [
      "@testing-library/jest-native/extend-expect"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!(@react-native|react-native|react-native-vector-icons)/)"
    ]
  }
}
